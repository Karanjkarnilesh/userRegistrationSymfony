{% include "base.html.twig" %}
{% block body %}
	<div class="container">


		{% for message in app.flashes('success') %}
			<div class="alert alert-success">
				{{ message }}
			</div>
		{% endfor %}
		<div>
			{{ form_start(form) }}
			{{ form_widget(form) }}
            <div class="mt-3">
            
            <input class="btn btn-primary" type="submit" name="submit" value="Login" >
            
            </div>
			{{ form_end(form) }}
		</div>
	{% endblock %}
